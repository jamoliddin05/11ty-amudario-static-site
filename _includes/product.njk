                <div class="carousel-cell-bottom">
                    <a href="../assets/img/section-photos/agrometeo_station_uz_01-1.jpg"
                       data-fancybox="gallery">
                        <img src="../assets/img/section-photos/agrometeo_station_uz_01-1.jpg" class="carousel-image-bottom"/>
                    </a>
                </div>

                <div class="carousel-cell-bottom">
                    <a href="../assets/img/section-photos/dashboard1-1.png" data-fancybox="gallery">
                        <img src="../assets/img/section-photos/dashboard1-1.png" class="carousel-image-bottom"/>
                    </a>
                </div>

                <div class="carousel-cell-bottom">
                    <a href="../assets/img/section-photos/dashboard1-1.png" data-fancybox="gallery">
                        <img src="../assets/img/section-photos/dashboard1-1.png" class="carousel-image-bottom"/>
                    </a>
                </div>

                <div class="carousel-cell-bottom">
                    <a href="../assets/img/section-photos/dashboard1-1.png" data-fancybox="gallery">
                        <img src="../assets/img/section-photos/dashboard1-1.png" class="carousel-image-bottom"/>
                    </a>
                </div>
            </div>
        </div>
    </section>

    <section id="meteostation-online">
        <div class="custom-container section-wrapper-ordinary">
            <div class="text-block">
                <div>
                    <h3 class="text-uppercase">{{ mySite.thirdSection.title}}</h3>
                    <ul>
                        {% for featureText in mySite.thirdSection.paragraphs %}
                            <li>
                                <p>{{ featureText.text }}</p>
                            </li>
                        {% endfor %}
                    </ul>
                </div>
            </div>
            <div class="image-block">
                <div id="meteostation-wrap">
                    <a href="../{{ mySite.thirdSection.imageOne }}"
                       data-fancybox="meteostation">
                        <img src="../{{ mySite.thirdSection.imageOne }}" class="mb-3"/>
                    </a>
                    <a href="../{{ mySite.thirdSection.imageTwo }}"
                       data-fancybox="meteostation">
                        <img src="../{{ mySite.thirdSection.imageTwo }}" class="mb-3"/>
                    </a>
                </div>
            </div>
        </div>
    </section>

    <section id="hardware-characteristics">
        <div class="custom-container section-wrapper-ordinary">
            <div class="image-block">
                <div id="meteostation-wrap">
                    <a href="../assets/img/section-photos/difference.png"
                       data-fancybox>
                        <img src="../assets/img/section-photos/difference.png" class="mb-3"/>
                    </a>
