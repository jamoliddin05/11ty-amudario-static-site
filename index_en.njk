            <p class="my-5">hello</p>
            <p class="my-5">test</p>
            <p class="my-5">test</p>
        </section>
    </main>
    <script>
        // This will log the full URL of the current page to the console
        console.log(window.location.href);
    </script>
</body>
<script src="assets/js/flickity.pkgd.min.js"></script>
<script src="assets/js/top_carousel.js"></script>
<script src="assets/js/sticky_navbar.js"></script>
<script src="assets/js/bootstrap.bundle.min.js"></script>
</html>